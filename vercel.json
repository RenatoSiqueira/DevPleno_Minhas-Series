{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/assets",
      "dest": "/public/assets"
    },
    {
      "src": "/images",
      "dest": "/public/images"
    },
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ]
}
